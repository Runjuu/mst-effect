{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react",
    "target": "ES2019"
  }
}
